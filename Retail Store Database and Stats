--Create a grocery store database

CREATE TABLE my_store (id INTEGER PRIMARY KEY, item TEXT, quantity INTEGER, price INTEGER, aisle INTEGER);

INSERT INTO my_store VALUES (1, "bubbles", 5, 3, 7);
INSERT INTO my_store VALUES (2, "socks", 10, 2, 4);
INSERT INTO my_store VALUES (3, "stickers", 4, 2.50, 7);

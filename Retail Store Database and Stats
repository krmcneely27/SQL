INSERT INTO my_store VALUES (6, "deodorant", 13, 4.50, 5);
INSERT INTO my_store VALUES (7, "pens", 20, 1.50, 3);
INSERT INTO my_store VALUES (8, "erasers", 15, 0.50, 3);
INSERT INTO my_store VALUES (9, "notebooks", 5, 3.50, 2);
INSERT INTO my_store VALUES (10, "toothbrush", 10, 2.50, 5);
INSERT INTO my_store VALUES (11, "dog treats", 4, 6.50, 6);
INSERT INTO my_store VALUES (12, "cat food", 4, 7, 6);
INSERT INTO my_store VALUES (13, "planner", 3, 7.50, 2);
INSERT INTO my_store VALUES (14, "chips", 10, 3.50, 1);
INSERT INTO my_store VALUES (15, "pencils", 20, 1.50, 3);

--display database ordered by price
SELECT * FROM my_store;
SELECT * FROM my_store ORDER BY price;

--what is the sum of all items in the store?
SELECT SUM(quantity) FROM my_store;
